  "scripts": {
    "build": "tsc",
    "lint": "tslint -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "yarn test && yarn lint",
    "release": "npx np",
    "go": "yarn build && node ."
  },
  "keywords": [],
  "files": [
    "lib/*.js",
    "lib/index.d.ts"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://davidbrownman.com"
  },
  "license": "ISC",
