      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "jest": "25.2.7",
    "ts-jest": "25.3.1",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.8.3"
  },
  "dependencies": {}
